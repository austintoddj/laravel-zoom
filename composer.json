{
	"name": "austintoddj/zoom",
	"description": "A Laravel admin panel",
	"keywords": ["laravel", "admin", "zoom"],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/austintoddj/zoom/issues",
		"source": "https://github.com/austintoddj/zoom"
	},
	"authors": [
		{
			"name": "Todd Austin",
			"email": "austin.todd.j@gmail.com"
		}
	],
	"require": {
		"php": "^7.1.3",
		"ext-json": "*",
		"illuminate/support": "~5.8.0"
	},
	"require-dev": {
		"mockery/mockery": "^1.1",
		"orchestra/testbench": "~3.7",
		"phpunit/phpunit": "^7.5"
	},
	"autoload": {
		"psr-4": {
			"Zoom\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Zoom\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"extra": {
		"laravel": {
			"providers": [
				"Zoom\\ZoomServiceProvider"
			]
		}
	},
	"config": {
		"sort-packages": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
